#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(j==n-1 || j==0 || (i==j && i<n/2) || (i==n/2 && j==n/2) || (i+j==n-1 && i<n/2)){
                printf("* ");
            }
            else{
                printf("  ");
                }
        }
        printf("\n");
    }
    
}
