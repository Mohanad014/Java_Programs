import java.util.Scanner;
public class Main {
  public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int a,b,c=0;
    while(n>0){
      int q=n%10;
      int fact=1;
      for(int i=1;i<=q;i++){
        fact=fact*i;
      }
      c+=fact;
      n=n/10;
    }
    
    System.out.println(c);
  }
}
