import java.util.*;
public class Main{
  public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int count=sc.nextInt();
    int c=0;
    System.out.print(a+" "+b + " " );
    for(int i=2;i<count;i++){
      c=a+b;
      System.out.print( c + " " );
    a=b;
    b=c;
  }
}
}
